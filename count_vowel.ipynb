   "source": [
    "Write a program to count the number of vowels in a given string using a for loop."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Aman mishra\n",
      "4\n"
     ]
    }
   ],
   "source": [
    "string = input('enter the string ')\n",
    "print(string)\n",
    "vowel_count = 0\n",
    "vowel = 'aeiouAEIOU'\n",
    "\n",
    "\n",
    "for str in string:\n",
    "    if str in vowel:\n",
    "        vowel_count += 1\n",
    "        \n",
    "print(vowel_count)"
   ]
  }
